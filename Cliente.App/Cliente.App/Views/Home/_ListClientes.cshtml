@model List<Cliente.DTO.Clientes.ClienteViewModel>
@{
    Layout = null;
}

<table class="table table-hover">
    <thead>
        <tr>
            <th>Nome</th>
            <th>CPF</th>
            <th>Celular</th>
            <th>Endereço</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var cli in Model)
        {
            <tr>
                <td>@cli.Nome</td>
                <td>@cli.CPF</td>
                <td>@cli.Celular</td>
                <td>@cli.Endereco</td>
            </tr>
        }
    </tbody>
</table>